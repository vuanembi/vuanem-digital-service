{
    "name": "vuanem-digital-service",
    "version": "1.0.0",
    "dependencies": {
        "@google-cloud/bigquery": "^6.1.0",
        "@google-cloud/functions-framework": "^3.1.3",
        "@google-cloud/secret-manager": "^4.2.0",
        "axios": "^1.3.2",
        "cors": "^2.8.5",
        "dayjs": "^1.11.7",
        "express": "^4.18.2",
        "joi": "^17.7.1",
        "json2csv": "^5.0.7",
        "knex": "^2.4.2",
        "lodash": "^4.17.21",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.4.0",
        "@types/json2csv": "^5.0.3",
        "@types/lodash": "^4.14.191",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.33.0",
        "jest": "^29.4.2",
        "ts-jest": "^29.0.5",
        "typescript": "^4.9.5"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testRegex": "./src/.*.test.ts$",
        "setupFiles": [
            "dotenv/config"
        ],
        "testTimeout": 540000
    },
    "eslintConfig": {
        "env": {
            "es2021": true,
            "node": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "ecmaVersion": "latest",
            "sourceType": "module"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "rules": {
            "@typescript-eslint/no-unused-vars": 0,
            "@typescript-eslint/no-explicit-any": 0
        }
    }
}
